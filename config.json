{
  "start": "today-2day",
  "interval": "6hour",
  "range": {
    "min": "today-week",
    "max": "today"
  },
  "branches": "autoland",
  "schedulers": [
    "seta",
    "relevant_tests",
    "bugbug_push_schedules"
  ],
  "config_db": {
    "filename": "config.sqlite",
    "upgrade": false
  },
  "hg_cache": {
    "use_cache": true,
    "hg": {
      "url": "https://hg.mozilla.org"
    },
    "repo": {
      "host": "http://localhost",
      "port": 9200,
      "$ref": "#es",
      "index": "repo",
      "type": "revision",
      "typed": false,
      "timeout": 300,
      "consistency": "one",
      "debug": false,
      "limit_replicas": false
    }
  },

  "destination": {
    "account_info": {
      "$ref": "file:///e:/moz-fx-dev-ekyle-treeherder-a838a7718652.json"
    },
    "dataset": "dev-scheduling",
    "table": "schedulers",
    "schema": {
      "push.id._i_": "integer",
      "push.date._t_": "time",
      "etl.timestamp._t_": "time"
    },
    "top_level_fields": {
      "push.id": "_push_id",
      "push.date": "_push_date",
      "etl.timestamp": "_etl_timestamp"
    },
    "partition": {
      "field": "push.date",
      "expire": "2year"
    },
    "id": {
      "field": "push.id",
      "version": "etl.timestamp"
    },
    "cluster": [
      "push.id"
    ],
    "sharded": true
  },
  "adr": {
    "verbose": 1,
    "url": "https://activedata.allizom.org/query",
    "cache": {
      "retention": 10080,
      "serializer": "json",
      "stores": {
//        "file": {
//          "driver":"seeded-file",
//          "path": ".cache",
//          "url": "https://s3-us-west-2.amazonaws.com/communitytc-bugbug/data/adr_cache.tar.zst",
//          "archive_relpath": "data/adr_cache",
//          "reseed_interval": 10080
//
//        },
        "file_cache": {
          "driver": "file",
          "path": ".cache"
        }
      }
    }
  },
  "constants": {
    "jx_bigquery.bigquery.DEBUG": true
  },
  "debug": {
    "trace": true
  }
}